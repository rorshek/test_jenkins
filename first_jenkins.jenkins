#!groovy

properties([disableConcurrentBuilds()])


pipeline {
	agent {
		label 'master'
	}

	triggers { pollSCM('* * * * *') }

	options {
		timestamps()
	}

	stages {
		stage("docker image") {
			steps {
				echo "Creating image"
				dir ('docker/toolbox') {
					sh 'docker build . '
				}

			}
		}
	}
}